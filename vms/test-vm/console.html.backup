    <script>
        let currentTab = 'terminal';
        let vmRunning = true;

        function startVM() {
            if (!vmRunning) {
                addTerminalLine('Starting VM...', 'output');
                setTimeout(() => {
                    vmRunning = true;
                    updateVMStatus('running');
                    addTerminalLine('‚úÖ VM started successfully', 'output');
                }, 1000);
            } else {
                addTerminalLine('‚ö†Ô∏è  VM is already running', 'warning');
            }
        }

        function stopVM() {
            if (vmRunning) {
                addTerminalLine('Stopping VM...', 'output');
                setTimeout(() => {
                    vmRunning = false;
                    updateVMStatus('stopped');
                    addTerminalLine('‚èπÔ∏è  VM stopped', 'output');
                }, 1000);
            } else {
                addTerminalLine('‚ö†Ô∏è  VM is already stopped', 'warning');
            }
        }

        function restartVM() {
            addTerminalLine('Restarting VM...', 'output');
            setTimeout(() => {
                addTerminalLine('‚úÖ VM restarted successfully', 'output');
            }, 1500);
        }

        function openConsole() {
            switchTab('vnc');
            connectVNC();
        }

        // Pure HTML5 Browser-based Display - No external dependencies!
        let vmDisplayReady = false;

        function connectVNC() {
            const vmDisplay = document.getElementById('vmDisplay');
            const status = document.getElementById('vncStatus');

            if (!termDisplay) return;

            // Create xterm.js instance for pure browser terminal
            if (window.Terminal) {
                xtermInstance = new Terminal({
                    cursorBlink: true,
                    fontSize: 14,
                    fontFamily: 'Courier New, monospace',
                    theme: {
                        background: '#000000',
                        foreground: '#00ff00',
                        cursor: '#00ff00'
                    },
                    rows: 30,
                    cols: 100
                });

                // Load fit addon
                const fitAddon = new FitAddon.FitAddon();
                xtermInstance.loadAddon(fitAddon);

                // Open terminal in the display
                xtermInstance.open(termDisplay);
                fitAddon.fit();

                // Update status
                status.textContent = 'üü¢ HTML5 Terminal Ready';
                status.className = 'vnc-status connected';

                // Welcome message
                xtermInstance.writeln('\x1b[1;32m‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó\x1b[0m');
                xtermInstance.writeln('\x1b[1;32m‚ïë      QuetzalCore VM - Pure HTML5 Browser Console          ‚ïë\x1b[0m');
                xtermInstance.writeln('\x1b[1;32m‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù\x1b[0m');
                xtermInstance.writeln('');
                xtermInstance.writeln('\x1b[1;36mVM ID:\x1b[0m test-vm-001');
                xtermInstance.writeln('\x1b[1;36mMemory:\x1b[0m 2048 MB (TPS, Compression, Ballooning)');
                xtermInstance.writeln('\x1b[1;36mvCPUs:\x1b[0m 2 cores');
                xtermInstance.writeln('\x1b[1;36mNetwork:\x1b[0m Bridge mode (en0)');
                xtermInstance.writeln('\x1b[1;36mStatus:\x1b[0m \x1b[1;32m‚úÖ RUNNING\x1b[0m');
                xtermInstance.writeln('');
                xtermInstance.writeln('\x1b[1;33müöÄ VM Boot Sequence:\x1b[0m');
                xtermInstance.writeln('[    0.000000] Linux version 5.15.0-quetzal (QuetzalCore Hypervisor)');
                xtermInstance.writeln('[    0.000000] Command line: BOOT_IMAGE=/boot/vmlinuz ro quiet');
                xtermInstance.writeln('[    0.012034] x86/fpu: Supporting XSAVE feature 0x001: FP');
                xtermInstance.writeln('[    0.012034] x86/fpu: Supporting XSAVE feature 0x002: SSE');
                xtermInstance.writeln('[    0.124589] Memory: 2048MB available');
                xtermInstance.writeln('[    0.234123] CPU: 2 cores detected');
                xtermInstance.writeln('[    0.456789] Network: virtio_net driver initialized');
                xtermInstance.writeln('[    0.567891] QuetzalCore: TPS enabled, 0 pages shared');
                xtermInstance.writeln('[    0.678912] QuetzalCore: Memory compression active');
                xtermInstance.writeln('[    0.789123] QuetzalCore: Balloon driver ready');
                xtermInstance.writeln('[    1.234567] \x1b[1;32m‚úÖ VM initialization complete\x1b[0m');
                xtermInstance.writeln('');
                xtermInstance.writeln('\x1b[1;36mQuetzalCore VM ready for workload\x1b[0m');
                xtermInstance.writeln('');
                xtermInstance.write('\x1b[1;32mroot@test-vm-001:~#\x1b[0m ');

                // Handle user input
                let currentLine = '';
                xtermInstance.onData(data => {
                    if (data === '\r') { // Enter key
                        xtermInstance.write('\r\n');
                        handleCommand(currentLine.trim());
                        currentLine = '';
                        xtermInstance.write('\x1b[1;32mroot@test-vm-001:~#\x1b[0m ');
                    } else if (data === '\u007F') { // Backspace
                        if (currentLine.length > 0) {
                            currentLine = currentLine.slice(0, -1);
                            xtermInstance.write('\b \b');
                        }
                    } else {
                        currentLine += data;
                        xtermInstance.write(data);
                    }
                });

                // Auto-resize on window resize
                window.addEventListener('resize', () => {
                    fitAddon.fit();
                });

                addTerminalLine('‚úÖ HTML5 terminal initialized', 'output');
            } else {
                // Fallback if xterm.js fails to load
                termDisplay.innerHTML = `
                    <div style="color: #0f0; font-family: monospace; padding: 20px;">
                        <h3>QuetzalCore VM Console</h3>
                        <p>VM ID: test-vm-001</p>
                        <p>Status: ‚úÖ RUNNING</p>
                        <p>Memory: 2048 MB</p>
                        <p>vCPUs: 2</p>
                        <p style="margin-top: 20px;">Pure browser-based VM - no local software required!</p>
                    </div>
                `;
                status.textContent = 'üü¢ Display Active';
                status.className = 'vnc-status connected';
            }
        }

        function handleCommand(cmd) {
            if (!xtermInstance) return;

            if (cmd === '') return;

            switch (cmd.toLowerCase()) {
                case 'help':
                    xtermInstance.writeln('Available commands:');
                    xtermInstance.writeln('  \x1b[1;36mhelp\x1b[0m       - Show this help message');
                    xtermInstance.writeln('  \x1b[1;36mstatus\x1b[0m     - Show VM status');
                    xtermInstance.writeln('  \x1b[1;36mtop\x1b[0m        - Show resource usage');
                    xtermInstance.writeln('  \x1b[1;36mps\x1b[0m         - List processes');
                    xtermInstance.writeln('  \x1b[1;36mdf\x1b[0m         - Show disk usage');
                    xtermInstance.writeln('  \x1b[1;36mifconfig\x1b[0m   - Show network config');
                    xtermInstance.writeln('  \x1b[1;36mclear\x1b[0m      - Clear screen');
                    break;
                case 'status':
                    xtermInstance.writeln('\x1b[1;32m‚úÖ VM Status: RUNNING\x1b[0m');
                    xtermInstance.writeln('Uptime: 00:15:23');
                    xtermInstance.writeln('Load average: 0.15, 0.08, 0.03');
                    break;
                case 'top':
                    xtermInstance.writeln('CPU Usage: 23%');
                    xtermInstance.writeln('Memory: 856 MB / 2048 MB (42%)');
                    xtermInstance.writeln('Processes: 87 total, 3 running');
                    break;
                case 'ps':
                    xtermInstance.writeln('PID   USER     COMMAND');
                    xtermInstance.writeln('1     root     /sbin/init');
                    xtermInstance.writeln('234   root     /usr/bin/sshd');
                    xtermInstance.writeln('456   www      nginx');
                    xtermInstance.writeln('789   app      python app.py');
                    break;
                case 'df':
                    xtermInstance.writeln('Filesystem     Size  Used Avail Use% Mounted on');
                    xtermInstance.writeln('/dev/vda1       20G  8.2G  11.8G  41% /');
                    break;
                case 'ifconfig':
                    xtermInstance.writeln('eth0: flags=4163<UP,BROADCAST,RUNNING>');
                    xtermInstance.writeln('      inet 192.168.1.100  netmask 255.255.255.0');
                    xtermInstance.writeln('      ether 52:54:00:12:34:56');
                    break;
                case 'clear':
                    xtermInstance.clear();
                    break;
                default:
                    xtermInstance.writeln(`\x1b[1;31mbash: ${cmd}: command not found\x1b[0m`);
                    xtermInstance.writeln('Type \x1b[1;36mhelp\x1b[0m for available commands');
            }
        }

        // Auto-connect when VNC tab is opened
        function switchTab(tab) {
            // Hide all panels
            document.getElementById('terminalPanel').classList.add('hidden');
            document.getElementById('vncPanel').classList.add('hidden');
            document.getElementById('logsPanel').classList.add('hidden');

            // Remove active class from all tabs
            document.querySelectorAll('.console-tab').forEach(t => t.classList.remove('active'));

            // Show selected panel
            const panels = {
                'terminal': 'terminalPanel',
                'vnc': 'vncPanel',
                'logs': 'logsPanel'
            };
            document.getElementById(panels[tab]).classList.remove('hidden');

            // Add active class to clicked tab
            event.target.classList.add('active');
            currentTab = tab;

            // Auto-connect VNC when tab is opened
            if (tab === 'vnc' && !vncSocket) {
                setTimeout(connectVNC, 100);
            }
        }

        function updateVMStatus(status) {
            const badge = document.getElementById('vmStatus');
            if (status === 'running') {
                badge.textContent = 'RUNNING';
                badge.className = 'status-badge status-running';
            } else {
                badge.textContent = 'STOPPED';
                badge.className = 'status-badge status-stopped';
            }
        }

        function addTerminalLine(text, type = 'output') {
            if (currentTab !== 'terminal') return;

            const terminal = document.getElementById('terminalPanel');
            const line = document.createElement('div');
            line.className = `terminal-line terminal-${type}`;
            line.textContent = text;
            terminal.appendChild(line);
            terminal.scrollTop = terminal.scrollHeight;
        }

        // Simulate resource usage updates
        function updateResources() {
            const cpuUsage = Math.floor(Math.random() * 40) + 10; // 10-50%
            const memUsage = Math.floor(Math.random() * 512) + 512; // 512-1024 MB

            document.getElementById('cpuUsage').textContent = cpuUsage + '%';
            document.getElementById('cpuBar').style.width = cpuUsage + '%';

            document.getElementById('memUsage').textContent = memUsage + ' MB';
            const memPercent = (memUsage / 2048) * 100;
            document.getElementById('memBar').style.width = memPercent + '%';
        }

        // Update resources every 2 seconds
        setInterval(updateResources, 2000);
        updateResources();

        // Add welcome message
        setTimeout(() => {
            addTerminalLine('Welcome to QuetzalCore VM Console', 'output');
            addTerminalLine('Type "help" for available commands', 'output');
        }, 500);
    </script>
</body>

</html>