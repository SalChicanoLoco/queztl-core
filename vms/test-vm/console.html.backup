        <div class="console-area">
            <div class="console-tabs">
                <div class="console-tab active" onclick="switchTab('terminal')">Terminal</div>
                <div class="console-tab" onclick="switchTab('vnc')">VNC Display</div>
                <div class="console-tab" onclick="switchTab('logs')">Logs</div>
            </div>

            <div id="terminalPanel" class="console-display">
                <div class="terminal-line terminal-prompt">root@test-vm-001:~#</div>
                <div class="terminal-line terminal-output">üöÄ Starting QuetzalCore Hypervisor</div>
                <div class="terminal-line terminal-output">VM: test-vm-001</div>
                <div class="terminal-line terminal-output">Memory: 2048 MB (with TPS, compression, ballooning)</div>
                <div class="terminal-line terminal-output">vCPUs: 2</div>
                <div class="terminal-line terminal-output">Status: Running</div>
                <div class="terminal-line terminal-output">‚úÖ VM booted successfully</div>
                <div class="terminal-line" style="margin-top: 10px;">
                    <span class="terminal-prompt">root@test-vm-001:~#</span>
                    <span style="color: white;">_</span>
                </div>
            </div>

            <div id="vncPanel" class="vnc-container hidden">
                <div class="vnc-status connected" id="vncStatus">üü¢ VM Display Active</div>
                <div id="vmDisplay"
                    style="width: 100%; height: 100%; background: #000; color: #0f0; font-family: 'Courier New', monospace; font-size: 14px; padding: 20px; overflow: auto; white-space: pre-wrap;">
                    Loading VM display...</div>
            </div>

            <div id="logsPanel" class="console-display hidden">
                <div class="terminal-line terminal-output">[2025-12-09 11:30:00] VM initialization started</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:01] Loading configuration from config.json
                </div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:01] VM ID: test-vm-001</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:01] Allocating 2048 MB memory</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:02] Allocating 2 vCPUs</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:02] Mounting disk image: disk.img</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:03] Configuring network bridge</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:03] Enabling KVM acceleration</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:04] Starting VirtIO drivers</div>
                <div class="terminal-line terminal-output">[2025-12-09 11:30:05] ‚úÖ VM started successfully</div>
            </div>
